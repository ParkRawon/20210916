<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.po.prj.member.service.MemberMapper">
	<select id="memberSearch" resultType="co.po.prj.member.vo.MemberVO">
		select * from member
		where id = #{id} and useyn = 'Y'  <!-- #{id} 전달인자의 변수명 -->
	</select>
</mapper>